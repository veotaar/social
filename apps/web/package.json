{
  "name": "@repo/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "generate-routes": "tsr generate",
    "check-types": "tsc --noEmit",
    "watch-routes": "tsr watch",
    "dev": "NODE_ENV=development bun watch-routes & bun --bun --watch src/index.html --port=3001",
    "start": "NODE_ENV=production bun server.ts",
    "start:dev": "NODE_ENV=production bun src/index.html --port=3001",
    "build": "bun generate-routes && bun run build.ts"
  },
  "dependencies": {
    "@dicebear/collection": "^9.2.4",
    "@dicebear/core": "^9.2.4",
    "@elysiajs/eden": "^1.4.5",
    "@tanstack/react-form": "^1.27.3",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router": "^1.141.1",
    "@tanstack/react-router-devtools": "^1.141.1",
    "@tanstack/react-virtual": "^3.13.13",
    "@tanstack/zod-adapter": "^1.141.1",
    "better-auth": "^1.4.6",
    "bun-plugin-tailwind": "^0.0.14",
    "clsx": "^2.1.1",
    "daisyui": "^5.5.13",
    "date-fns": "^4.1.0",
    "immer": "^10.2.0",
    "lucide-react": "^0.552.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@repo/config": "workspace:*",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/router-cli": "^1.141.1",
    "@tanstack/router-plugin": "^1.141.1",
    "@types/bun": "latest",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "elysia": "^1.4.18"
  }
}
