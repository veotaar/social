# Postgres
POSTGRES_USER=your_postgres_username
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_postgres_db_name

# Redis
REDIS_PASSWORD=your_redis_password

# Drizzle Gateway
DRIZZLE_GATEWAY_PASSWORD=your_drizzle_gateway_password

# API
LOG_LEVEL=info
BETTER_AUTH_SECRET=your_super_secret_key
BETTER_AUTH_URL=http://localhost:3000

# S3 (MinIO) - External service
S3_ACCESS_KEY_ID=your-s3-access-key
S3_SECRET_ACCESS_KEY=your-s3-secret-key
S3_BUCKET_NAME=your-s3-bucket-name
S3_ENDPOINT=https://your-s3-endpoint.example.com

CORS_ORIGIN=http://localhost:3001
TRUSTED_ORIGINS=http://localhost:3001,http://localhost:3000

# Web Frontend Build Arg
# This URL is baked into the web build at compile time
BUN_PUBLIC_API_URL=http://localhost:3000
